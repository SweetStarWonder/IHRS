         </children>
      </GridPane>
      <HBox alignment="CENTER" layoutX="72.0" layoutY="199.0" prefHeight="36.0" prefWidth="217.0" spacing="30.0">
         <children>
            <Button fx:id="confirmButton" layoutX="139.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleConfirmButton" text="确定" />
            <Button fx:id="cancelButton" layoutX="214.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleCancelButton" text="取消" />
         </children>
      </HBox>
   </children>
</AnchorPane>
